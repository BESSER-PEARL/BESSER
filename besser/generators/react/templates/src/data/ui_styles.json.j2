{
  "styles": [
    {
      "id": "style_text_title",
      "styling": {
        "size": {
          "width": "100%",
          "height": "auto",
          "padding": "10px",
          "margin": "10px",
          "font_size": "24px",
          "unit_size": "px"
        },
        "position": {
          "p_type": "relative",
          "alignment": "center",
          "top": "10px",
          "left": "auto",
          "right": "auto",
          "bottom": "auto",
          "z_index": 1
        },
        "color": {
          "background_color": "transparent",
          "text_color": "#c12828",
          "border_color": "none"
        }
      }
    },
    {
      "id": "style_chart_line",
      "styling": {
        "size": {
          "width": "100%",
          "height": "400px"
        },
        "position": {
          "p_type": "relative"
        },
        "color": {
          "background_color": "#ffffff",
          "bar_color": "#4CAF50"
        }
      }
    },
    {
      "id": "style_chart_bar",
      "styling": {
        "size": {
          "width": "100%",
          "height": "400px"
        },
        "position": {
          "p_type": "relative"
        },
        "color": {
          "background_color": "#ffffff",
          "bar_color": "#3498db"
        }
      }
    }
  ],
  "layouts": [
    {
      "id": "layout_default",
      "layout_type": "flex",
      "orientation": "vertical",
      "gap": "20px",
      "padding": "20px",
      "alignment": "center",
      "wrap": true
    }
  ]
}
